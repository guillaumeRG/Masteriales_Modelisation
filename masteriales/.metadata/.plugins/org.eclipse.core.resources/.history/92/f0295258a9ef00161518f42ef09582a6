
public class CalcNi {
	final static int NB_STATIONS =8;
	final static int Dmax =300;
	
	static int[][] coordMat= new int[NB_STATIONS][2];
	public static void main(String[] args) {
		CalcNi obj = new CalcNi();
		obj.exempleMatrix();
	}
	public float dist(Coord c){
		float dist = 0;
		return dist;
	}
	public void exempleMatrix(){
		for(int i=0;i<NB_STATIONS;i++){
			coordMat[i][0]=i;
			coordMat[i][1]=i;
			System.out.println("exemple : x y:"+coordMat[i][0]+" "+coordMat[i][1]);
			
		}
	}
}
